<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hibernate.entity.Student" table="Student">

		<meta attribute="class-description">
			This class contains the employee detail.
		</meta>

		<id name="sno" type="int" column="sno" />
		<!-- <id name = "id" type = "int" column = "id"> <generator class="native"/></id> -->
		<property name="sname" column="student_name" type="string" />
		<property name="smarks" column="smarks" type="double" />

	</class>
</hibernate-mapping>